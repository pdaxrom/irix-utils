+  if (r < 0.) {
+    r = -r;
+  }
+
+  if (r < 1.) {
+    for (; result >= r; i++) {
+      result *= .1;
+    }
+
+    i = -i;
+  } else {
+    for (; result <= r; i++) {
+      result *= 10.;
+    }
+
+    --i;
+  }
+
+  return i;
