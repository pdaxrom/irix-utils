+    /* put the sign ? */
+    if (is_signed && number < 0) {
+      output[i++] = '-';
+    }
+
+    output[i] = '\0';
+    
+    /* reverse every thing */
+    for (i--, j = 0; j < i; j++, i--) {
+      char tmp = output[i];
+      output[i] = output[j];
+      output[j] = tmp;
+    }
+  } else {
+    precision = precision < 0 ? 1 : precision;
+    for (i = 0; i < (size_t)precision && i < output_size; i++) {
+      output[i] = '0';
+    }
+    output[i] = '\0';
+  }
